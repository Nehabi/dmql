<div>
    <link rel="stylesheet" href="css/main.css">
    <h3>Ecommerce</h3>
    <form  action="/erdiagram" method="get">
        <input type="submit" value="Er Diagram"></input>
    </form>
    <form  action="/table" method="get">
        <input type="submit" value="View Tables"></input>
    </form>
    <form  action="/query" method="get">
    <h4>Enter query to process </h4>
    <textarea type="text" id="query" name="query" rows="10" cols="100"></textarea>
    <br>
    <input type="submit" value="Submit"></input>
    </form>
    <h4><%= data.err.name %></h4>
    <table>
        <% if(data.result){
            var flag=1;
         for(const obj of data.result){ %>
         <tr>
          <% 
          if(flag==1){
            for(const key in obj) { %>
           <th scope="row"><%= key %></th>
          <% } 
            flag=0}%>
         </tr>
         <tr>
         <% for(const key in obj) { %>
          <td><%= obj[key] %></td>
         <% } %>
         </tr>
        <% }
         } %> 
    </table>
</div>

       
